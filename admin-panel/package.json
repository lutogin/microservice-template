{
  "name": "chat-bot-diesel",
  "version": "0.0.1",
  "description": "Chat bot for diesel selling on Node.js for Telegram",
  "main": "./build/main.js",
  "author": "Burdin Bogdan <burdinbogdan@gmail.com> (https://github.com/burdinbogdan)",
  "license": "MIT",
  "engines": {
    "node": "10.15",
    "npm": "6.4.3",
    "yarn": "1.13"
  },
  "scripts": {
    "start": "bnr start:development",
    "start:development": "bnr start:development",
    "start:production": "bnr start:production",
    "build": "backpack build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "bnr test",
    "test:watch": "bnr test --watch",
    "test:ci": "bnr test --ci --colors --silent --coverage",
    "prettier": "prettier --write",
    "prettier:all": "prettier \"**/*.js\" --write",
    "livereload": "node ./src/helpers/livereload-watcher.js"
  },
  "betterScripts": {
    "start:development": {
      "command": "concurrently \"yarn livereload\" \"yarn backpack\" ",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:production": {
      "command": "node ./build/main.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --runInBand --forceExit --config ./internals/jest/jest.config.json",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "yarn lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@logger": "git+https://my-work-token:WvSDTFfcidGLj2xzh5xG@gitlab.com/burdinbogdan/modules-logger.git#v0.0.3",
    "@sentry/node": "4.6.5",
    "agenda": "2.0.2",
    "axios": "0.18.0",
    "bcryptjs": "2.4.3",
    "better-npm-run": "0.1.1",
    "body-parser": "1.18.3",
    "bootstrap-select": "1.13.8",
    "cloudinary": "1.13.2",
    "cookie-parser": "1.4.4",
    "csv-parser": "2.2.0",
    "dotenv-safe": "6.1.0",
    "express": "4.16.4",
    "express-handlebars": "3.0.2",
    "express-session": "^1.15.6",
    "handlebars": "4.1.1",
    "handlebars-dateformat": "1.1.1",
    "handlebars-helpers": "0.10.0",
    "handlebars-intl": "1.1.2",
    "handlebars.moment": "1.0.4",
    "helper-date": "1.0.1",
    "http-status-codes": "1.3.2",
    "joi": "14.3.1",
    "json2csv": "4.3.5",
    "jsonwebtoken": "8.5.1",
    "method-override": "3.0.0",
    "moment": "2.29.2",
    "mongoose": "5.4.19",
    "mongoose-auto-increment": "5.0.1",
    "multiparty": "4.2.1",
    "node-emoji": "1.10.0",
    "node-telegram-bot-api": "0.30.0",
    "nodejs-base64": "1.0.3",
    "ramda": "0.26.1",
    "request": "2.88.0",
    "sha1": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-preset-es2015": "6.24.1",
    "backpack-core": "0.8.3",
    "concurrently": "^4.1.0",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "8.1.5",
    "livereload": "^0.7.0",
    "prettier": "1.16.4",
    "supertest": "4.0.2"
  }
}
